{
  "name": "todo-socket",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run next-dev\" \"npm run json-server\" \"npm run socket-server\"",
    "next-dev": "next dev",
    "json-server": "json-server --watch src/db.json --port 3002 --no-cors",
    "socket-server": "node src/server.js",
    "build": "next build",
    "start": "concurrently \"next start\" \"npm run json-server\" \"npm run socket-server\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "fabric": "^6.5.1",
    "json-server": "^0.16.3",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "react-toastify": "^10.0.6",
    "reselect": "^5.1.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/json-server": "^0.14.7",
    "@types/node": "^20.17.8",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-flatpickr": "^3.8.11",
    "concurrently": "^9.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prettier": "3.3.3",
    "react-flatpickr": "^3.10.13",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
